{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "Host": "Information",
      "Function": "Information",
      "RingFunctionAppHost": "Information",
      "Azure.Core": "Error",
      "default": "Warning"
    }
  }
}